{
  "name": "@vue-cdk/storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "start-storybook --ci --port 9999",
    "build": "build-storybook -c .storybook -o dist"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "@vue-cdk/virtualized-list": "^0.1.0",
    "@vue-cdk/virtualized-list-css": "^0.1.0",
    "@vue-cdk/link": "^0.1.0",
    "@vue-cdk/portal": "^0.1.0",
    "@vue-cdk/match-media": "^0.1.0",
    "@vue-cdk/popover": "^0.1.0",
    "@vue-cdk/popover-themes": "^0.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.0-beta.14",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-backgrounds": "5.3.0-beta.14",
    "@storybook/addon-viewport": "5.3.0-beta.14",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@storybook/addon-storysource": "5.3.0-beta.2",
    "babel-preset-vue": "^2.0.2",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10",
    "@storybook/vue": "^5.2.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
